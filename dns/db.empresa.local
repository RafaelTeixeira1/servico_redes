$TTL 86400
@   IN  SOA ns1.empresa.local. admin.empresa.local. (
        2025040101 ; Serial (AAAAMMDDnn)
        3600       ; Refresh
        1800       ; Retry
        604800     ; Expire
        86400 )    ; Negative Cache TTL
;
@       IN NS ns1.empresa.local.
ns1     IN A 192.168.1.2
www     IN A 192.168.1.7
intranet IN A 192.168.1.7
ldap    IN A 192.168.1.4
ftp     IN A 192.168.1.6

